type Query {
    getAllMovies: [Movie]
    getMovieById(id: ID): Movie
}

type Movie {
    id: ID
    name: String
    actor: Actor
    ratings: Int
}

type Actor {
    id: ID
    actor_name: String
}

type Mutation {
    addMovie(movie: MovieInput): Movie
    addActor(name: String): Actor

    modifyActor(id: ID, name: String): Actor
}

input MovieInput {
    name: String
    ratings: Int
    actorId: Int
}

input ActorInput {
    id: ID
    name: String
}